public class MyClass {
    public static void main(String args[]) {
        
        int[] arr = {1,2,3,4,5};
        int product = 1;
        for(int num : arr) {
            product *= num;
        }
        System.out.println(product);
        for(int i = 0; i< arr.length; i++) {
            arr[i] = product / arr[i];
        }
        
        for(int num : arr) {
             System.out.println(num);
            
        }

       
    }
}
